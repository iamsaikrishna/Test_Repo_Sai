<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<context:annotation-config/>

<bean id="emp" class="com.model.Employee">
<property name="empid" value="458"></property>
<property name="empname" value="saikrishna"></property>
<property name="salary" value="1000"></property>
<!--  <property name="address" ref="addr"></property>-->
 </bean>

<bean id="addr" class="com.model.Address">
<property name="streetname" value="Islampet"></property>
<property name="city" value="tenali"></property>
<property name="state" value="AP"></property>
<property name="pincode" value="522261"></property>
</bean>


    
    
 </beans>